{
    "manifest_version": 3,
    "background": {
        "service_worker": "service_worker.js",
        "type": "module"
    },
    "action": {
        "default_icon": "img/icon128.png",
        "default_title": "Chrome Extension Socket",
        "default_popup": "popup.html"
    },    
    "description": "Example of Chrome Extension Socket.js",
	"icons": {
        "16": "img/icon16.png",
        "48": "img/icon48.png",
        "128": "img/icon128.png"
    },
    "name": "Chrome Extension Socket.js Example", 
    "permissions": [
        "scripting",
        "tabs"
    ],
    "host_permissions": [
        "http://localhost/isoprocess*"
    ],
    "version": "1.0.0"
}